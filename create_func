    class Student:
    
    def __init__(self,stdno,name,course):
        self.stdno = stdno
        self.name = name
        self.course = course
        lststdno=[]
        lstname=[]
        lstcourse=[]
        lststdno.append(stdno)
        lstname.append(name)
        lstcourse.append(course)
        print("Student No : " ,lststdno)
        print("Student Name : ", lstname)
        print("Studen Course : " ,lstcourse)
